#include <Servo.h>
Servo myServo;
int servoPin = 9;
String servoPos;
int pos;

void setup() {
Serial.begin(9600);
myServo.attach(servoPin);
myServo.write(90);
}

void loop() {
if (Serial.available() == 0){
}
    servoPos = Serial.readStringUntil(‘\r’);
    pos = servoPos.toInt();
    myServo.write(pos);
    delay(50);
